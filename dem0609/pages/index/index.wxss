page{
  margin:0;
  padding:0;
  box-sizing: border-box; 
}
.welcome{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 100;
  background-color:rgba(0,0,0,.5);
}
.come{
  position: relative; 
  width: 75%;
  height: 230rpx;
  left: 0;
  right: 0;
  top: 50%;
  bottom: 0;
  margin: auto;
  margin-top:-120rpx;
  padding: 70rpx 0;
  z-index: 9999;
  background-color: #fff;
  border-radius: 30rpx;
}
.welcome_title{
  text-align: center;
  font-size: 40rpx;
}
.welcome_content{
  padding: 30rpx 0;
  text-align: center;
  color: #bababa;
  border-bottom: 1px solid #dfd9d9;
}
.come>button{
  font-size: 40rpx;
  color:#fff;
  background-color: #fff;
}
.come>button::after{
  border:none;
}




.bannerImg{
  width:100%;
  max-height: 450rpx;
}
.indexWrap{
  position: absolute;
  width:100%;
  margin: 0 5%;
  margin-top: -80rpx;
}
.indexNavLi{
  width:26%;
  margin: 0 2%;
  padding: 0;
  border:none;
  float: left;
  height: 150rpx;
  line-height: 150rpx;
  background-color: rgb(255,127,91);
  text-align: center;
  color: #fff;
  border-radius: 10rpx;
}
button[disabled]{
  background-color: rgba(255,127,91,.5)!important;
  color:#fff!important;
  border:none!important;
}
.indexCon{
  margin-top: 20rpx;
  width:90%;
  line-height: 43rpx;
  color:rgb(255,127,91);
}
.content{
  padding:10rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.barImg{
  width: 90%;
  margin-top: 40rpx;
  height: 40rpx;
} 